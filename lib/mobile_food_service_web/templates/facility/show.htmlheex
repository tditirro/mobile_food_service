<h1>Show Facility</h1>

<ul>

  <li>
    <strong>Id:</strong>
    <%= @facility.id %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @facility.name %>
  </li>

  <li>
    <strong>Food items:</strong>
    <%= @facility.food_items %>
  </li>

  <li>
    <strong>Type:</strong>
    <%= @facility.type %>
