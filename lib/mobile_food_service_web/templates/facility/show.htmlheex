  <li>
    <strong>Status:</strong>
    <%= @facility.status %>
  </li>

  <li>
    <strong>Schedule:</strong>
    <%= @facility.schedule %>
  </li>

  <li>
    <strong>Schedule url:</strong>
    <%= @facility.schedule_url %>
